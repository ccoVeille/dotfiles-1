defaults
	domain snow.nullroute.eu.org
	logfile ~/.cache/msmtp.log
	tls on
	tls_trust_file /etc/ssl/certs/ca-certificates.crt

account nullroute
	from grawity@nullroute.eu.org
	host equal.cluenet.org
	port 587
	auth gssapi
	user grawity
	tls_trust_file /etc/ssl/certs/nullroute.pem

account gmail
	from grawity@gmail.com
	host smtp.gmail.com
	port 587
	auth plain
	user grawity@gmail.com

account default : nullroute
