# k5login.rules: rules for ~/.k5login generation by dist/update-k5login
# vim: ft=conf

+cluenet
	*.cluenet.org
+cluenet-admin
	radian.cluenet.org
	virgule.cluenet.org
+nullroute
	*.nullroute.eu.org
	*.home
+nathan7
	*.nathan7.eu

grawity@CLUENET.ORG
	+cluenet
	+nathan7
	+nullroute
grawity/admin@CLUENET.ORG
	+cluenet
grawity@NATHAN7.EU
	+cluenet !+cluenet-admin
	+nathan7
grawity@NULLROUTE.EU.ORG
	*
grawity/admin@NULLROUTE.EU.ORG
	+nullroute
